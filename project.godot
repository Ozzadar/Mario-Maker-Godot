[application]

config/name="Platformer Mission"
run/main_scene="res://src/Levels/MainMenuLevel/MainMenuLevel.tscn"
config/icon="res://icon.png"

[autoload]

Config="*res://config/Config.gd"
GlobalState="*res://src/GlobalState.gd"
APIIntegration="*res://src/Utils/APIIntegration.gd"
Tilemap="*res://src/Tilemap.gd"
InputManager="*res://src/Utils/InputManager.gd"
Statics="*res://src/Statics.gd"
Data="*res://config/database.cdb"

[display]

window/size/width=1920
window/size/height=1080
window/size/borderless=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "FontAwesome", "castledb-godot" )

